<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

        <display-name>Archetype Created Web Application</display-name>
        <servlet>
            <servlet-name>MyServletName</servlet-name>
            <servlet-class>webapp.ee.HelloServlet</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>MyServletName</servlet-name>
            <url-pattern>/hello</url-pattern>
        </servlet-mapping>

        <servlet>
            <servlet-name>ServletXML</servlet-name>
            <servlet-class>webapp.ee.EmployeeServlet</servlet-class>
        </servlet>
        <servlet-mapping>
            <servlet-name>ServletXML</servlet-name>
            <url-pattern>/xml</url-pattern>
        </servlet-mapping>

    <servlet>
        <servlet-name>ServletAuthors</servlet-name>
        <servlet-class>webapp.ShowAuthorsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAuthors</servlet-name>
        <url-pattern>/ShowAuthorsServlet</url-pattern>
    </servlet-mapping>

</web-app>